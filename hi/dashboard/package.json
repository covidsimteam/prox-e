{
    "author": "Risav Karna, Saujan Ghimire, Kripa Bhattarai",
    "name": "@nepwork/covidsimteam-dashboard",
    "version": "0.0.1",
    "license": "GPL-3.0-or-later",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nepwork/admin-ui.git"
    },
    "bugs": {
        "url": "https://github.com/nepwork/admin-ui/issues"
    },
    "description": "COVSIMTEAM - NEPAL Dashboard",
    "homepage": "https://github.com/nepwork/admin-ui#readme",
    "main": "index.html",
    "keywords": [
        "dashboard",
        "angular",
        "nebular",
        "covidsimteam"
    ],
    "scripts": {
        "ng": "ng",
        "conventional-changelog": "conventional-changelog",
        "stage-all": "git add .",
        "commit": "npx git-cz",
        "start": "ng serve",
        "build": "ng build",
        "build:prod": "npm run build -- --prod",
        "build:stag": "npm run build -- --configuration staging",
        "test": "ng test",
        "test:coverage": "rimraf coverage && npm run test -- --code-coverage",
        "lint": "ng lint",
        "lint:fix": "ng lint ngx-admin-demo --fix",
        "lint:styles": "stylelint ./src/**/*.scss",
        "lint:ci": "npm run lint && npm run lint:styles",
        "pree2e": "webdriver-manager update --standalone false --gecko false",
        "e2e": "ng e2e",
        "docs": "compodoc -p src/tsconfig.app.json -d docs",
        "docs:serve": "compodoc -p src/tsconfig.app.json -d docs -s",
        "prepush": "npm run lint:ci",
        "release:changelog": "npm run conventional-changelog -- -p angular -i CHANGELOG.md -s",
        "bundle:phobia": "source-map-explorer dist/*.js{,.map}",
        "postinstall": "ngcc --properties es2015 es5 browser module main --first-only --create-ivy-entry-points --tsconfig './src/tsconfig.spec.json'"
    },
    "dependencies": {
        "@akveo/ng2-completer": "^9.0.1",
        "@angular-devkit/build-ng-packagr": "^0.1000.6",
        "@angular/animations": "^9.0.4",
        "@angular/cdk": "^9.1.2",
        "@angular/common": "^9.0.4",
        "@angular/compiler": "^9.0.4",
        "@angular/core": "^9.0.4",
        "@angular/forms": "^9.0.4",
        "@angular/platform-browser": "^9.0.4",
        "@angular/platform-browser-dynamic": "^9.0.4",
        "@angular/router": "^9.0.4",
        "@angular/service-worker": "^9.0.4",
        "@asymmetrik/ngx-leaflet": "^7.0.1",
        "@nebular/auth": "^5.1.0",
        "@nebular/eva-icons": "5.0.0",
        "@nebular/security": "^5.0.0",
        "@nebular/theme": "5.0.0",
        "@swimlane/ngx-charts": "^13.0.2",
        "angular2-chartjs": "0.4.1",
        "bootstrap": "4.3.1",
        "chart.js": "^2.9.3",
        "classlist.js": "1.1.20150312",
        "component-emitter": "^1.3.0",
        "core-js": "^3.6.5",
        "echarts": "^4.0.2",
        "eva-icons": "^1.1.3",
        "intl": "1.2.5",
        "ionicons": "2.0.1",
        "leaflet": "^1.6.0",
        "leaflet.fullscreen": "^1.6.0",
        "leaflet.markercluster": "^1.4.1",
        "nebular-icons": "1.1.0",
        "ng2-smart-table": "^1.6.0",
        "ngx-csv-parser": "0.0.2",
        "ngx-echarts": "^5.1.2",
        "node-sass": "^4.12.0",
        "normalize.css": "6.0.0",
        "pace-js": "1.0.2",
        "polylabel": "^1.1.0",
        "pouchdb": "^7.2.2",
        "pouchdb-authentication": "^1.1.3",
        "roboto-fontface": "0.8.0",
        "rxjs": "6.5.4",
        "rxjs-compat": "6.3.0",
        "socicon": "3.0.5",
        "style-loader": "^1.1.3",
        "tslib": "^1.10.0",
        "typeface-exo": "0.0.22",
        "web-animations-js": "^2.3.2",
        "zone.js": "~0.10.2"
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "config": {
        "commitizen": {
            "path": "@commitlint/prompt"
        }
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.1000.5",
        "@angular/cli": "^9.0.4",
        "@angular/compiler-cli": "^9.0.4",
        "@angular/language-service": "9.0.4",
        "@compodoc/compodoc": "^1.1.11",
        "@fortawesome/fontawesome-free": "^5.2.0",
        "@types/d3-color": "1.0.5",
        "@types/echarts": "^4.6.4",
        "@types/jasmine": "2.5.54",
        "@types/jasminewd2": "2.0.3",
        "@types/leaflet": "^1.5.12",
        "@types/leaflet.fullscreen": "^1.6.0",
        "@types/leaflet.markercluster": "^1.4.2",
        "@types/node": "^12.11.1",
        "@types/polylabel": "^1.0.5",
        "@types/pouchdb": "^6.4.0",
        "@types/dotenv": "^4.0.3",
        "codelyzer": "^5.1.2",
        "commitizen": "^4.1.2",
        "conventional-changelog-cli": "1.3.4",
        "husky": "0.13.3",
        "jasmine-core": "2.6.4",
        "jasmine-spec-reporter": "4.1.1",
        "karma": "1.7.1",
        "karma-chrome-launcher": "2.1.1",
        "karma-cli": "1.0.1",
        "karma-coverage-istanbul-reporter": "1.3.0",
        "karma-jasmine": "1.1.0",
        "karma-jasmine-html-reporter": "0.2.2",
        "npm-run-all": "4.0.2",
        "protractor": "5.1.2",
        "rimraf": "2.6.1",
        "stylelint": "7.13.0",
        "ts-node": "3.2.2",
        "tslint": "^5.7.0",
        "tslint-language-service": "^0.9.9",
        "typescript": "3.7.5"
    }
}
